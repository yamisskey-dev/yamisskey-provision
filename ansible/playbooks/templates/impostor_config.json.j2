{
  "Server": {
    "PublicIp": "{{ impostor_public_ip }}",
    "PublicPort": 22023,
    "ListenIp": "0.0.0.0",
    "ListenPort": 22023
  },
  "HttpServer": {
    "Enabled": true,
    "ListenIp": "0.0.0.0",
    "ListenPort": 22023
  },
  "AntiCheat": {
    "Enabled": true,
    "BanIpFromGame": true
  },
  "Compatibility": {
    "AllowFutureGameVersions": true,
    "AllowVersionMixing": true
  },
  "Plugins": {
    "LoadDirectory": "plugins",
    "LibraryDirectory": "libraries"
  },
  "Serilog": {
    "MinimumLevel": "Verbose",
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs/log.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  }
}